}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nom_Suc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Afo_Suc)
        </th>
