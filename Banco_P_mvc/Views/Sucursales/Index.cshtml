            @Html.DisplayFor(modelItem => item.Pro_Suc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dep_Suc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dir_Suc)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Cod_Suc }) |
            @Html.ActionLink("Details", "Details", new { id=item.Cod_Suc }) |
